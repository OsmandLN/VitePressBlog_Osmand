{
  "devDependencies": {
    "@types/node": "^20.11.5",
    "generate-license-file": "^4.1.1",
    "sass": "^1.69.7",
    "vitepress": "^1.0.2"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "build": "vitepress build && npm run oss",
    "oss": "generate-license-file --input package.json --output docs/.vitepress/dist/THIRD-PARTY-NOTICES.txt"
  },
  "dependencies": {
    "vue": "^3.4.5",
    "vuetify": "^3.11.2"
  }
}
